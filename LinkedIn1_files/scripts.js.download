jQuery('label, div').keyup(function(e) {
	if(e.keyCode !== 13) {
		return
	}

	if ( jQuery(e.target).parents('.accordion').length > 0 ) {
		jQuery(e.target).trigger('click')
	}

	if ( jQuery(e.target).parents('.tabs__nav').length > 0 ) {
		tabsNavSwitchTo(jQuery(e.target).parent())
	}

	if ( jQuery(e.target).parents('.checkbox-label').length > 0 ) {
		jQuery(e.target).parents('.checkbox-label').find('input[type="checkbox"]').trigger('click')
	}



})


function setLoading( element, text ) {
	var template = '<div>'+text+'</div><div class="loading-dots"><span></span><span></span><span></span></div>';
	element.addClass('loading')
	element.parents('form').addClass('loading')
	element.attr('data-text', element.html());
	element.html(template)
}

function removeLoading( element ) {
		element.parents('form').removeClass('loading')
		element.removeClass('loading');
		element.html(element.attr('data-text'));
}


