(function($) {

$(document).on('change', '.main-cookie-setting', function() {
  var checked = $(this).is(':checked');
  var selectors = $(this).parents('.cookie-settings-section').find('table input[type="checkbox"]');
  if(checked) {
    selectors.attr('checked', 'checked');
  } else {
    selectors.removeAttr('checked')
  }
})

$(document).on('ready', function() {
  $.each($('.main-cookie-setting'), function(i, item) {
    setting = $(item);
    if(setting.parents('.cookie-settings-section').find('.cookie-setting:checked').length > 0) {
      setting.attr('checked', 'checked')
    }
  })
})


$(document).on('click', '.cookie-settings-details-toggle', function() {
  $(this).parents('.cookie-settings-section').find('.cookie-settings-details').slideToggle();
})

$(document).on('change', '.cookie-setting', function() {
  var checked = $(this).is(':checked');
  var selectorCount = $(this).parents('.cookie-settings-section').find('table input[type="checkbox"]').length;
  var checkedCount = $(this).parents('.cookie-settings-section').find('table input[type="checkbox"]:checked').length;

  if(checkedCount === 0) {
    $(this).parents('.cookie-settings-section').find('.main-cookie-setting').removeAttr('checked')
  }

  if(checkedCount === selectorCount) {
    $(this).parents('.cookie-settings-section').find('.main-cookie-setting').attr('checked', 'checked')
  }


})


function update_cookie_settings(cookie_settings, successCallback, beforeSendCallback) {
  $.ajax({
    url: kinsta.ajaxurl,
    type: 'post',
    data: {
      action: 'save_cookie_settings',
      cookie_settings: cookie_settings
    },
    beforeSend: beforeSendCallback,
    success: successCallback
  });
}


$(document).on('click', '.accept-all-cookies-button', function() {
  var button = $(this);
  successCallback = function() {
    window.location.href += "#settings_saved";
    location.reload();
    return false;
  };
  beforeSendCallback = function() {
    setLoading( button, 'Accepting' );
  }
  update_cookie_settings('all', successCallback, beforeSendCallback);
})



$(document).on('click', '.save-cookie-settings-button', function() {
  var settings = $('.cookie-setting');
  var button = $(this);
  var cookie_settings = {};
  $.each(settings, function(i, item) {
    setting = $(item);
    cookie_settings[setting.attr('name')] = setting.is(':checked');
  })

  successCallback = function() {
    window.location.href += "#settings_saved";
    location.reload();
    return false;
  };
  beforeSendCallback = function() {
    setLoading( button, 'Saving settings' );
  }

  update_cookie_settings(cookie_settings, successCallback, beforeSendCallback)

  return false;
})


})(jQuery);

