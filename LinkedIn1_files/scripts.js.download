		plugin.fieldEventHandler = function(event) {
			var field = jQuery(event.target);

			var fieldValidatorsForField = plugin.getValidatorsForField(field)
			var fieldValidatorsForEvent = plugin.getFieldValidatorsForEvent(fieldValidatorsForField, event);

			if(fieldValidatorsForEvent.length === 0) {
				plugin.handleFieldReset(field);
