(function($) {
	$.kinstaToc = function(element, options) {
		var plugin = this;
		plugin.settings = {}

		var defaults = {};
		var $element = $(element);
		var state = 'normal'

		plugin.init = function() {
			plugin.settings = $.extend({}, defaults, options);
			$element.css({width: plugin.getTocWidth() })
			$element.css({paddingLeft: plugin.getLeftOffset() })
			$element.css({visibility: 'visible'})
			window.addEventListener( 'scroll', plugin.debouncedScrollHandler, false);
			window.addEventListener( 'resize', plugin.debouncedResizeHandler, false);
			gumshoe.init({
			  activeClass: 'current',
			  offset: 60,
			});
			new SimpleBar(document.getElementById('kinsta-toc'))

		}

		plugin.debouncedResizeHandler = debounce(function() {
			$element.css({width: plugin.getTocWidth() })
			$element.css({paddingLeft: plugin.getLeftOffset() })
		}, 12);

		plugin.debouncedScrollHandler = debounce(function() {
			plugin.handleStickyness();
		}, 12);

		plugin.handleStickyness = function() {
			if(state === 'stuckTop') {
				hero = document.getElementById('kinsta-hero').getBoundingClientRect();
				footer = document.getElementById('site-footer').getBoundingClientRect();
				if(hero.height + hero.y + 56 >= 0 ) {
					state = 'normal';
					$element.removeClass('stuckBottom');
					$element.removeClass('stuckTop');
					$element.addClass('normal');
					$element.css({top: 'auto', bottom: 'auto' })
				}
				if(window.innerHeight >= footer.top ) {
					footerBar = document.getElementById('site-footer-bar').getBoundingClientRect();
					state = 'stuckBottom',
					$element.removeClass('stuckTop');
					$element.removeClass('normal');
					$element.addClass('stuckBottom');
					$element.css({bottom: footer.height + footerBar.height, top: 'auto'})
				}

			}

			if(state === 'stuckBottom') {
				hero = document.getElementById($element.attr('id')).getBoundingClientRect();
				if(hero.top > 0) {
					$element.removeClass('stuckBottom');
					$element.addClass('stuckTop');
					$element.css({top: 56, bottom: 'auto' })
					state = 'stuckTop'
				}

			}


			if(document.getElementById('kinsta-toc').getBoundingClientRect().y <= 56 && state !== 'stuckTop' && state !== 'stuckBottom') {
				$element.removeClass('normal');
				$element.addClass('stuckTop');
				$element.css({top: 56, bottom: 'auto' })
				state = 'stuckTop'
			}
		}

		plugin.getTocWidth = function() {
			return document.getElementById('kinsta-toc-lead').getBoundingClientRect().x - 40;
		}

		plugin.getLeftOffset = function() {
			return $('#site-header .container:first .row:first').offset().left
		}

