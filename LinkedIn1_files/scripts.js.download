function ajaxCheckFreeEmail(email) {
  var deferred = jQuery.Deferred();

  if( kinsta.env !== 'production' ) {
    if( email === 'notfree@test.com' ) {
      deferred.reject({
        name: 'freeEmail',
        isValid: false,
        message: kinsta.translations.validationFreeEmail
      })
    } else {
      deferred.resolve({
        name: 'freeEmail',
        isValid: true,
      })
    }
    return deferred.promise();
  }

  jQuery.ajax({
    url: kinsta.ajaxurl,
    type: 'post',
    dataType: 'json',
    data: {
      action: 'check_mykinsta_email',
      email: email,
    },
    success : function(response) {
      if(response.valid === true) {
        deferred.resolve({
          name: 'freeEmail',
          isValid: true,
        })
      } else {
        deferred.reject({
          name: 'freeEmail',
          isValid: false,
          message: kinsta.translations.validationFreeEmail
        })
      }
    }
  })

  return deferred.promise();

}






function ajaxCheckVat(vat) {
  var deferred = jQuery.Deferred();

  if( kinsta.env !== 'production' ) {
    if( vat === 'HU22320351' || vat === 'HU22250351' ) {
      deferred.resolve({
        name: 'vat',
        isValid: true,
      })
    } else {
      deferred.reject({
        name: 'vat',
        isValid: false,
        message: kinsta.translations.validationVatInvalidVies
      });
    }
    return deferred.promise();
  }


  jQuery.ajax({
    url: 'https://apilayer.net/api/validate',
    type: 'get',
    dataType:'json',
    data: {
      access_key: kinsta.vatlayer_api_key,
      vat_number: vat,
    },
    success: function(result) {
      if(result.database !== 'ok') {
        deferred.resolve({
          name: 'vat',
          isValid: true,
        })
      } else {
        if( result.valid === true ) {
          deferred.resolve({
            name: 'vat',
            isValid: true,
          })
        } else {
          deferred.reject({
            name: 'vat',
            isValid: false,
            message: kinsta.translations.validationVatInvalidVies
          });
        }
      }
    }

  })

  return deferred.promise();

}

