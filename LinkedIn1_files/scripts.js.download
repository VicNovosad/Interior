  var name = jQuery(this).attr('name');
  var thisInput = this;
  var otherContent = jQuery('input[name="' + name +'"]').not(thisInput).parents('.accordion').find('.accordion__content');

  var thisContent = jQuery(this).parents('.accordion').find('.accordion__content');

