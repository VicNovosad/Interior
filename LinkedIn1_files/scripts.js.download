			})).always(function() {
				$element.attr('data-initialValidation', 'true');
				var hasError = false;
				jQuery.each(arguments, function(i, argument) {
					if(argument[0] && argument[0].results) {
						hasError = true;
						plugin.handleFieldError(argument[0].field, argument[0].results);
					}
				})
				plugin.removeFormLoading();
				if(!hasError) {
					$element.trigger('submit', [true])
				}
      });
		}

		plugin.handleFieldReset = function(field) {
			plugin.setControlState(field, 'neutral');
			plugin.hideFieldErrorMessage(field);
		}

		plugin.handleFieldSuccess = function(field, results, hasOtherEventValidators, event) {
			if(event.target === '#stripeCard') {
				if( event.complete === false ) {
					plugin.handleFieldReset(field);
				} else {
					plugin.setControlState(field, 'valid');
					plugin.hideFieldErrorMessage(field);
				}
			} else {
				if(!hasOtherEventValidators && event.type !== 'keyup' || field.attr('id') === 'repeatPassword' && event.type === 'keyup' ) {
					if(field.attr('id') === 'repeatPassword' && field.val() === '') {
						return
					}
					plugin.setControlState(field, 'valid');
					plugin.hideFieldErrorMessage(field);
				}
			}

			if(jQuery(event.target).attr('id') === 'vat' && jQuery('#vat').val() === '') {
				plugin.setControlState(field, 'neutral');
			}

			if(field.attr('id') === 'vat' && typeof emitVatChange === 'function' ) {
				emitVatChange({ countryCode: getCurrentCountry(), vatNumber: getCurrentVatNumber(), userType: 	getCurrentUserType() });
			}
		}

		plugin.handleFieldError = function(field, results) {
			plugin.setControlState(field, 'invalid');
			if($element.attr('data-initialValidation') === 'true' || field.attr('data-alwaysValidate') === 'true' || field.attr('data-alwaysValidate') === results[0].name ) {
				plugin.showFieldErrorMessage(field, results);
			}
			if(field.attr('id') === 'vat' && typeof emitVatChange === 'function' ) {
				emitVatChange({ countryCode: getCurrentCountry(), vatNumber: getCurrentVatNumber(), userType: 	getCurrentUserType() });
			}

		}

		plugin.init = function() {
			if( $element.find('#stripeCard').length > 0 ) {
				stripe = Stripe(kinsta.stripe_public_key);
				stripeElements = stripe.elements();
				card = stripeElements.create('card');
				card.mount('#stripeCard');
				card.addEventListener('change', function(event) {
					event.type = 'change';
					event.target = '#stripeCard'
					plugin.fieldEventHandler(event)
				})
			}

			if( jQuery("[name='user-country']") ) {
				jQuery("[name='user-country']").chosen({width: '100%'})
			}

      plugin.fields = $element.find('[data-validations]');
      plugin.fields.on('change keyup blur', function(event) {
				plugin.fieldEventHandler(event)
			})

      $element.on('submit', function(event, checked) {

				plugin.setFormLoading();
				if( checked !== true) {
					plugin.submitFormHandler(event);
					event.stopPropagation();
        	return false;
				}
      })

		}


		/* UI Stuff */

		plugin.hideFieldErrorMessage = function(field) {
			var control = field.parents('.form-control');
			var errorMessage = control.find('.control-message');
			if(!errorMessage.hasClass('control-message--hint')) {
				errorMessage.remove();
			}
			errorMessage.removeClass('control-message--error')
		}

		plugin.showFieldErrorMessage = function(field, results) {
			var message = plugin.getErrorMessage(field, results[0].name, results[0].message)
			var control = field.parents('.form-control');
			var errorMessage = control.find('.control-message');
			if( errorMessage.length > 0 ) {
				errorMessage.html(message)
				errorMessage.addClass('control-message--error')
			} else {
				errorMessage = jQuery('<div></div>');
				errorMessage.addClass('control-message');
				errorMessage.addClass('control-message--error')
				errorMessage.html(message);
				control.append(errorMessage)
			}
		}

		plugin.setControlState = function( field, state ) {
			var control = field.parents('.form-control');
			control.attr('data-state', state)
		}

    plugin.setFormLoading = function () {
			$element.addClass('loading')
      var button = $element.find('.submit')
      var loadingText = 'Loading';
      if( button.attr('data-loadingtext')) {
        loadingText = button.attr('data-loadingtext');
      }
      button.attr('data-defaulttext', button.text())

    	var template = '<div>'+loadingText+'</div><div class="loading-dots"><span></span><span></span><span></span></div>';
    	button.addClass('loading')
    	$element.addClass('loading')
      $element.find('input,textarea,select').attr('disabled', 'disabled')
    	button.html(template)
      button.attr('disabled', 'disabled')
    }

    plugin.removeFormLoading = function ( element ) {
			$element.removeClass('loading');
      var button = $element.find('.submit');
  		$element.removeClass('loading');
  		button.removeClass('loading');
  		button.html(button.attr('data-defaulttext'));
      $element.find('input,textarea,select').removeAttr('disabled');
      button.removeAttr('disabled');
