			}

			var deferred = jQuery.Deferred();


			var hasOtherEventValidators = fieldValidatorsForEvent.length !== fieldValidatorsForField.length;
			var localValidatorsForEvent = fieldValidatorsForEvent.filter(function(validator) {
				return validator.type === 'local'
			});
			var remoteValidatorsForEvent = fieldValidatorsForEvent.filter(function(validator) {
				return validator.type === 'remote' && event.type !== 'keyup'
			});

			plugin.log('---------------------------------');
		  plugin.log('%cValidation Event', "font-weight: bold; color:#5333ed");
		  plugin.log("%cEvent:", "font-weight: bold", event.type);
		  plugin.log("%cField:", "font-weight: bold", field.attr('id'));
		  plugin.log("%cValidations:", "font-weight: bold", fieldValidatorsForEvent.length);

			if( event.type === 'submit' ) {
				$.when.apply(jQuery, fieldValidatorsForEvent.map(function(validator) {
					return validator.validate(field, event)
				})).done(function() {
					deferred.resolve()
				}).fail(function() {
					var results = arguments;
					deferred.reject({
						field: field,
						event: event,
						field: field,
						results: results
					})
					Object.keys(results).forEach(function(key, i) {
						plugin.log('    ' + results[key].name + ': %cInvalid', "font-weight: bold; color:#DB0F13");
					})
				})
			} else {
				$.when.apply(jQuery, localValidatorsForEvent.map(function(validator) {
					return validator.validate(field, event)
				})).done(function() {
					var results = arguments;

					Object.keys(results).forEach(function(key, i) {
						plugin.log('    ' + results[key].name + ': %cValid', "font-weight: bold; color:#05AD85");
					})
				}).fail(function() {
					var results = arguments;
					plugin.handleFieldError(field, results);
					Object.keys(results).forEach(function(key, i) {
						plugin.log('    ' + results[key].name + ': %cInvalid', "font-weight: bold; color:#DB0F13");
					})
				}).then(function() {
					$.when.apply(jQuery, remoteValidatorsForEvent.map(function(validator) {
						return validator.validate(field, event)
					})).done(function() {
